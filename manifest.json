{
	"manifest_version": 2,
	"name": "My Extension",
  	"version": "1.2",

	"browser_action": {
    		"default_icon": "icon.png",
    		"default_popup": "popup.html",
		"default_title": "Youtube Search"
	},

	"oauth2": {
		"client_id": "REDACTED", //Go to Google API manager. OAUTH ID Name: "Chrome Client 1". Created Apr 5th, 2017. Type: "Chrome App"
		"scopes": [
		  "https://www.googleapis.com/auth/youtube.readonly"
		]
	},
	
	"background": {
		"scripts": ["background.js"]
	},
	
	"permissions": [
		"storage", 
		"tabs", 
		"identity",
		"identity.email",
		"https://www.googleapis.com/*"
	]
}
